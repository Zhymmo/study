//int main()
//{
//	int arr[] = { 1,2,3,4,5 };
//	int(*p1)[] = arr;
//	int(*(p2[]))[] = { p1,0 };
//	return 0;
//}
//void qsort(void* base,size_t nmemb,size_t size,int (*compar)(const void*, const void*));
int main()
{
	(*(void(*)())0)();
	//void(*)()-数组指针，指向的是无参数无输出的函数。
	//()0-把0强制转换为函数指针。
	//*-把函数指针0解应用得到函数
	//---()-使用函数，没有参数
	void (*signal(int, void(*)(int)))(int);
	//signal(int, void(*)(int))—参数为整形和函数指针的函数signal
	//void (*)(int)函数的返回类型为函数指针，函数参数为整数，无返回值。
	return 0;
}
