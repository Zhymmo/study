int removeDuplicates(int* nums, int numsSize)
{
    if(numsSize==0)
    return 0;
int* left=nums;
int* right=nums+1;
int count=0;
while(right<nums+numsSize)
{
    if(*right==*left)
    {
        right++;
    }
    else
    {
        left++;
        count++;
        *left=*right;
        right++;
    }
    
}
return count+1;
}
